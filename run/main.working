func KeplerSys(c *gin.Context) {
	command := c.Params.ByName("command")
	log.Critical("[HandleSystem] [%v]", command)

	switch command {
	case "shutdown":
		{
			c.Writer.Write([]byte("Shutting down..."))
			c.Writer.Flush()
			c.Request.Close = true
		}
		break
	}
}

