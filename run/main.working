// Middleware

func SystemCb() gin.HandlerFunc {
	return func(c *gin.Context) {
		command := c.Params.ByName("command")

		switch command {
		case "shutdown":
			{
				// Wait until all filters/handlers are done
				c.Next()

				// Notify quit
				close(quit)
			}
			break
		}
	}
}

